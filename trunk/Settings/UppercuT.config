  <!-- PROJECT VARIABLES -->
	<property name="project.name" value="Bombali" overwrite="false" />
  <property name="path_to_solution" value="." overwrite="false" />
	<property name="repository.path" value="http://bombali.googlecode.com/svn/" overwrite="false" />
	<property name="company.name" value="FerventCoder Software" overwrite="false" />
  <property name="dirs.db.project" value="__DATABASE_FOLDER_NAME__" overwrite="false" />
  <property name="dirs.report.project" value="__REPORT_PROJECT_FOLDER__" overwrite="false" />
  <property name="dirs.documentation" value="docs" overwrite="false" />
  <property name="app.drop.folder" value="Bombali" overwrite="false" />

